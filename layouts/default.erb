<!DOCTYPE HTML>
<html lang="en">
<!-- START HTML -->
<!-- HEAD -->
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js" defer></script>
<script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js" defer></script>
<script src="/assets/js/activeswitcher.js" defer></script>
<link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" type="text/css">
<link href="/assets/css/faskatech.css" rel="stylesheet" type="text/css">
<%= get_metas_for_head %>
</head> <!-- END HEAD  -->


<!-- BODY -->
<body>

<a id="gplusauthorattribution"  href="https://plus.google.com/+FabioScaccabarozzi88?rel=author">Google Plus profile</a>
<!-- Google Analytics with in-link tracking --> <%= render 'google_analytics' %> <!-- END Google Analytics script -->

<!-- WRAPPER to help with footer vertical alignment -->
<div id="wrapper">

<!-- NAVBAR --> <%= render 'navbar' %> <!-- END NAVBAR -->

<!-- MAIN -->
<div id="main">

<!-- CONTAINER -->
<div class="container">

<!-- ROW -->
<div class="row-fluid">

<!-- CONTENT DIV -->
<div class="col-sm-12 col-md-9">
<!-- CONTAINER -->
<div class="container-fluid">

<%= yield %>

</div> <!-- CONTAINER -->
</div> <!-- CONTENT DIV -->

<div class="col-sm-12 col-md-3" id="sidebarWrapper">
<!-- SIDEBAR --> <%= render 'sidebar' %> <!-- END SIDEBAR -->
</div>

</div> <!-- END ROW -->

</div> <!-- END CONTAINER -->

</div> <!-- END MAIN -->

</div> <!-- END WRAPPER -->

<!-- FOOTER --> <%= render 'footer' %> <!-- END FOOTER -->

</body> <!-- END BODY -->
<!-- END HTML -->
</html>
